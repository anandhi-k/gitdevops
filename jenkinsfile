
pipeline{
   environment{

    environment {
      JAVA_HOME = "myjdk"
      mvnHome = "mymaven"
      mvnCMD = "${mvnHome}/bin/mvn"
     }


    agent any
    stages{
     stage('checkout'){
        git 'https://github.com/edureka-devops/DevOpsClassCodes.git'
        
     }
     stage('compile'){
        script {
            sh "${mvnCMD} compile"
            echo "code is compiled..."
        }
        
     }
     stage('package'){
         script {
            
            sh "${mvnCMD} package"
            echo "Package is done....."
        }   
    }
  }
}
