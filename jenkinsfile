
pipeline{
    tools {
      maven 'mymaven'
      jdk 'myjdk'
    }
    agent any
    stages{
     stage('checkout'){
      steps{
        git 'https://github.com/edureka-devops/DevOpsClassCodes.git'
	echo "code is pulled from git"
        }
     }
     stage('compile'){
      steps{
        script {
              sh "mvn compile"
                     
            echo "code is compiled..."
        }
       }
     }
     stage('package'){
       steps{
         script {
            
            sh "${mvnCMD} package"
            echo "Package is done....."
        }   
       }
    }
}
}
